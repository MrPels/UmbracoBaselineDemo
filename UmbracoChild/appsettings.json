{
  "$schema": "appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "uSync": {
    "Settings": {
      "LockRoot": false,
      "ImportAtStartup": "Settings",
      "Folders": [
        "~/../UmbracoBaseline/uSync/Root",
        "uSync/v15"
      ]
    },
    "Sets": {
      "Default": {
        "Handlers": {
          "ContentHandler": {
            "Enabled": false
          },
          "MediaHandler": {
            "Enabled": false
          }
        }
      }
    }
  },
  "Umbraco": {
    "CMS": {
      "Global": {
        "Id": "553fac2b-1c05-465d-950d-5959914a86af",
        "SanitizeTinyMce": true
      },
      "Content": {
        "AllowEditInvariantFromNonDefault": true,
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        }
      },
      "Unattended": {
        "UpgradeUnattended": true
      },
      "Security": {
        "AllowConcurrentLogins": false
      }
    }
  },
  "ConnectionStrings": {
    "umbracoDbDSN": "",
    "umbracoDbDSN_ProviderName": ""
  }
}